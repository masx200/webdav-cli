{
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.com"
    },
    "name": "@masx200/webdav-cli",
    "version": "2.0.4",
    "description": "A simple zero-configuration command-line webdav server",
    "license": "MIT",
    "keywords": [
        "cli",
        "command",
        "webdav",
        "server"
    ],
    "bin": {
        "webdav-cli": "dist/webdav-cli.cli.js"
    },
    "preferGlobal": true,
    "main": "dist/index.js",
    "scripts": {
        "start": "node ./dist/webdav-cli.cli.js --username root --password root",
        "fix": "rm -rf node_modules && rm package-lock.json && npm install",
        "build": " tsc -p tsconfig.build.json",
        "format": "prettier --write \"**/*.ts\" **/*.js *.json *.md",
        "lint": "eslint 'lib/**/*.ts' --fix"
    },
    "devDependencies": {
        "@types/figlet": "^1.5.1",
        
        "@types/minimist": "^1.2.1",
        "@types/node": "^15.0.2",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^4.22.1",
        "@typescript-eslint/parser": "^4.22.1",
        "eslint": "^7.26.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.22.1",
        "hosted-git-info": "4.0.2",
        
        "lodash": "4.17.21",
        "prettier": "^2.3.0",
        "reflect-metadata": "^0.1.13",
        
        "supertest": "^6.1.3",
        
        "ts-loader": "^9.1.2",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.4"
    },
    "dependencies": {
        "chalk": "^4.1.1",
        "figlet": "^1.5.0",
        "ini": "2.0.0",
        "minimist": "^1.2.5",
        "webdav-server": "^2.6.2"
    },
    "directories": {
        "test": "test"
    },
    "repository": "git+https://github.com/masx200/webdav-cli.git",
    "author": "masx200 <masx200@qq.com>",
    "bugs": {
        "url": "https://github.com/masx200/webdav-cli/issues"
    },
    "homepage": "https://github.com/masx200/webdav-cli#readme"
}
